<%- include('partials/header'); -%>

<H2>DELETE an existing vaccination record</H2>

<!-- HTML Form (wrapped in a .bootstrap-iso div) -->
<div class="bootstrap-iso">
 <div class="container-fluid">
  <div class="row">
   <div class="col-md-6 col-sm-6 col-xs-12">
    <form action="/deletevacc" method="DELETE">
     <div class="form-group">
      <label class="control-label requiredField" for="id">
       Enter unique FHIR resource ID for vaccination event record
       <span class="asteriskField">
        *
       </span>
      </label>
      <div class="input-group">
       <div class="input-group-addon">
        <i class="fa fa-user-md">
        </i>
       </div>
       <input class="form-control" id="id" name="id" placeholder="12341234" type="text"/>
      </div>
     </div>
     <div class="form-group">
      <div>
       <button class="btn btn-info" name="submit" type="submit">
        Delete vaccination event
       </button>
      </div>
     </div>
    </form>
   </div>
  </div>
 </div>
</div>

<%- include('partials/footer'); -%>